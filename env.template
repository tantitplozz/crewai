# Payment Testing Mode
PAYMENT_TEST_MODE=true
SKIP_OTP=true

# Session Configuration
SESSION_TIMEOUT=3600
MAX_CONCURRENT_SESSIONS=5

# Notification Settings
NOTIFICATION_ERRORS_ONLY=false
